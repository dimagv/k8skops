{
    "rules": [
        {
            "rulePriority": 1,
            "description": "Delete untagged images every day",
            "selection": {
                "tagStatus": "untagged",
                "countType": "sinceImagePushed",
                "countUnit": "days",
                "countNumber": 1
            },
            "action": {
                "type": "expire"
            }
        },
        {
            "action": {
                "type": "expire"
            },
            "selection": {
                "countType": "imageCountMoreThan",
                "countNumber": 3,
                "tagStatus": "tagged",
                "tagPrefixList": [
                    "master"
                ],
                "countUnit": "days"
            },
            "description": "No more than 3 images for master branch",
            "rulePriority": 2
        }
    ]
}